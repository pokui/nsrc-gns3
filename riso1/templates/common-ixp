IXP_V6DEC="$(( 65536 - IXP ))"
IXP_V6BLOCK="2001:DB8:`printf "%04X" "$IXP_V6DEC"`"
IXP_V4SRV="100.127.$(( (IXP-1)*2 ))"
IXP_V4IXP="100.127.$(( (IXP-1)*2+1 ))"

SR_AS="$(( IXP+130 ))"
RS_AS="$(( 65535-IXP ))"

GROUP_OFFSET=$(( (IXP-1)*NGROUPS ))
FIRST_GROUP=$(( GROUP_OFFSET+1 ))
LAST_GROUP=$(( GROUP_OFFSET+NGROUPS-1 ))
